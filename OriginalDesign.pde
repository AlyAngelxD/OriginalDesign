int time = 0;
void setup()
{
  size(500, 500);
  frameRate(2);
}
void draw()
{
  background(100);
  halo();
  wings();
  heart();
  if (time < 2) {
    time = time + 1;
  }
  else {
    time = time - 1;
  }
}
void halo()
{
  //yellow
  stroke(255,240,110);
  strokeWeight(5);
  noFill();
  ellipse(250,190+time,100,25);
}
void heart()
{
  noStroke();
  //red
  fill(250, 170, 180);
  //right half of heart
  bezier(250, 250+time, 300, 175+time, 425, 275+time, 250, 350+time);
  //left half of heart
  bezier(250, 250+time, 500-300, 175+time, 500-425, 275+time, 250, 350+time);
}
void wings()
{
  //ref img:
  //data:image/jpeg;base64,
  //simple reg img:
  // https://thumb7.shutterstock.com/display_pic_with_logo/3777605/551794405/stock-vector-cute-angel-wings-and-nimbus-vector-cartoon-illustration-on-pink-background-551794405.jpg
  //white
  fill(255);
  noStroke();
  //right wing
  ellipse(350,260+time,150,40);
  ellipse(350,285+time,120,35);
  ellipse(350,310+time,80,30);
  //left wing
  ellipse(500-350,260+time,150,40);
  ellipse(500-350,285+time,120,35);
  ellipse(500-350,310+time,80,30);
}